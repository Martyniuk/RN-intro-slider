{
  "name": "IntroSlider",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start-clean": "yarn start --reset-cache",
    "clean": "watchman watch-del-all && npm cache clean --force && yarn cache clean",
    "clean-port": "kill -9 $(lsof -ti tcp:8081)",
    "ios-clean-build": "rm -rf ios/build/",
    "ios": "react-native run-ios --simulator='iPhone X'",
    "android": "react-native run-android",
    "commitmsg": "commitlint -E GIT_PARAMS",
    "test": "jest"
  },
  "dependencies": {
    "react": "16.8.3",
    "react-native": "0.59.9",
    "react-native-app-intro-slider": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/runtime": "^7.4.5",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@lectrum/eslint-config-core": "1.1.15",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "eslint-plugin-react-hooks": "^1.6.0",
    "eslint-plugin-react-native": "^3.6.0",
    "jest": "^24.8.0",
    "metro-react-native-babel-preset": "^0.54.1",
    "prettier": "^1.18.2",
    "prettier-eslint": "^8.8.2",
    "react-test-renderer": "16.8.3"
  },
  "jest": {
    "preset": "react-native"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts"
    ]
  }
}
